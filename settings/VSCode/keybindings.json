    },
    {
        "key": "ctrl+2",
        "command": "workbench.action.findInFiles",
        "when": "isWindows"
    },
    {
        "key": "ctrl+3",
        "command": "-workbench.action.focusThirdEditorGroup",
        "when": "isWindows"
    },
    {
        "key": "ctrl+3",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active && isWindows"
    },
    {
        "key": "ctrl+3",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active && !gitlens:disabled && config.gitlens.keymap == 'chorded' && isWindows"
    },
    {
